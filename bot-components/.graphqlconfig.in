{
  "projects": {
    "GitHub": {
      "schemaPath": "schema.json",
      "extensions": {
        "endpoints": {
          "default": {
            "url": "https://api.github.com/graphql",
            "headers": {
              "Authorization": "Bearer %%TOKEN%%"
            }
          }
        }
      }
    }
  }
}
